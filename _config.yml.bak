title: 流年笙歌的小窝
subtitle: 一个兴趣使然的无名小站
description: 一个无趣的人罢了
keywords: null
author: 流年笙歌
language: zh-CN
timezone: Asia/Shanghai
url: https://2671066.xyz
permalink: ':year/:month/:day/:title/'
permalink_defaults: null
pretty_urls:
  trailing_index: true
  trailing_html: true
source_dir: source
public_dir: public
tag_dir: tags
archive_dir: archives
category_dir: categories
code_dir: downloads/code
i18n_dir: ':lang'
skip_render: null
new_post_name: ':title.md'
default_layout: post
titlecase: false
external_link:
  enable: true
  field: site
  exclude: ''
filename_case: 0
render_drafts: false
post_asset_folder: false
relative_link: false
future: true
highlight:
  enable: false
  line_number: true
  auto_detect: false
  tab_replace: ''
  wrap: true
  hljs: false
prismjs:
  enable: true
  preprocess: true
  line_number: true
  tab_replace: ''
index_generator:
  path: ''
  per_page: 12
  order_by: '-date'
default_category: uncategorized
category_map: null
tag_map: null
meta_generator: true
date_format: YYYY-MM-DD
time_format: HH:mm:ss
updated_option: mtime
per_page: 12
pagination_dir: page
include: null
exclude: null
ignore: null
theme: matery
deploy:
  type: git
  repository: git@github.com:Raysamatoken/Raysamatoken.github.io.git
  branch: main
  
search:
  path: search.xml
  field: post

permalink_pinyin:
  enable: true
  separator: '-' # default: '-'

githubEmojis:
  enable: true
  className: github-emoji
  inject: true
  styles:
  customEmojis:
  
live2d:
  enable: true
  pluginModelPath: assets/
  model:
    use: live2d-widget-model-shizuku  #模板目录，在node_modules里
  display:
    position: left
    width: 200
    height: 400
  mobile:
    show: false  #是否在手机进行显示